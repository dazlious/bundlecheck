{
  "name": "bundlecheck",
  "version": "0.3.1",
  "description": "Check your application's build size and take code-splitting into account",
  "main": "index.js",
  "homepage": "https://dazlious.github.io/bundlecheck",
  "repository": {
    "type": "git",
    "url": "https://github.com/dazlious/bundlecheck.git"
  },
  "author": "Michael Duve <mduve@designmail.net>",
  "license": "MIT",
  "keywords": [
    "bundlesize",
    "performance",
    "measure",
    "check",
    "build",
    "artifact",
    "optimization"
  ],
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "bin": {
    "bundlecheck": "bin/bundlecheck"
  },
  "files": [
    "bin/",
    "src/!(*.test.js)"
  ],
  "engines": {
    "node": ">=8.3.0",
    "npm": ">=5.3.0"
  },
  "scripts": {
    "start": "nodemon index.js",
    "test": "jest",
    "lint": "eslint -c .eslintrc.js --ext .js .",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/git": "^8.0.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "semantic-release": "^16.0.1"
  },
  "dependencies": {
    "arg": "^4.1.2",
    "chalk": "^3.0.0",
    "esm": "^3.2.25",
    "fast-glob": "^3.1.1",
    "gzip-size": "^5.1.1",
    "lodash": "^4.17.21",
    "uuid": "^3.3.3"
  },
  "resolutions": {
    "minimist": "^1.2.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
